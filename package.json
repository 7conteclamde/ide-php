{
  "name": "languageserver-php",
  "main": "./lib/main",
  "version": "0.1.0",
  "description": "Provide PHP language support via felixbecker PHP Language Server",
  "repository": "https://github.com/atom/languageserver-php",
  "license": "MIT",
  "engines": {
    "atom": ">=1.17.0"
  },
  "atomTestRunner": "./test/runner",
  "dependencies": {
    "atom-languageclient": "^0.1.0",
    "vscode-jsonrpc": "^3.2.0"
  },
  "consumedServices": {
    "linter-indie": {
      "versions": {
        "2.0.0": "consumeLinterV2"
      }
    },
    "status-bar": {
      "versions": {
        "^1.0.0": "consumeStatusBar"
      }
    },
    "nuclide-datatip.provider": {
      "versions": {
        "0.0.0": "consumeDatatip"
      }
    }
  },
  "providedServices": {
    "autocomplete.provider": {
      "versions": {
        "2.0.0": "provideAutocomplete"
      }
    },
    "nuclide-outline-view": {
      "versions": {
        "0.0.0": "provideOutlines"
      }
    },
    "nuclide-definition-provider": {
      "versions": {
        "0.0.0": "provideDefinitions"
      }
    },
    "nuclide-find-references.provider": {
      "versions": {
        "0.0.0": "provideFindReferences"
      }
    }
  }
}
